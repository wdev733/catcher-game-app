{
  "name": "catcher-game-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "format": "prettier --write \"./**/*.ts\" \"./**/*.tsx\" \"./**/*.scss\" ./**/*.json README.md .prettierrc",
    "format:check": "prettier --check .",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "tsc": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --silent",
    "service:generateCatcher": "npx @rtk-query/codegen-openapi ./scripts/generateCatcherServices.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{tsx,ts}": [
      "yarn run lint:fix"
    ],
    "./**/*.{tsx,ts,css}": [
      "yarn run format"
    ]
  },
  "dependencies": {
    "@netlify/plugin-nextjs": "^4.37.4",
    "@reduxjs/toolkit": "^1.9.5",
    "babel-plugin-styled-components": "^1.12.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.30.0",
    "konva": "^9.2.0",
    "next": "^13.3.0",
    "prettier": "^2.8.8",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-game-engine": "^1.2.0",
    "react-hydration-provider": "^1.0.2",
    "react-konva": "^18.2.10",
    "react-redux": "^8.0.7",
    "redux": "^4.2.1",
    "sass": "^1.32.12",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@rtk-query/codegen-openapi": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^14.14.28",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.17",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.27",
    "redux-mock-store": "^1.5.4",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.5",
    "whatwg-fetch": "^3.6.2"
  }
}
